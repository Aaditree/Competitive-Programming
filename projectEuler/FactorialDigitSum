import java.io.*;
import java.util.Scanner;
public class Main{

     public static void main(String []args){
  Scanner sc=new Scanner(System.in);
  
  int n=sc.nextInt();
  int fact=1;
  for(int i=1;i<=n;i++)
  {
      fact=fact*i;
  }
  int sum=0;
  n=fact;
  while(n!=0)
  {
      int r=n%10;
      sum=sum+r;
      n=n/10;
  }
  System.out.println(sum);
       }
}
