import java.io.*;
import java.util.Scanner;
public class Main{

     public static void main(String []args){
  Scanner sc=new Scanner(System.in);
  
  int sum=sc.nextInt();
  int r=1;
      for(int i=1;i<=sum;i++)
      for(int j=1;j<=sum;j++)
      for(int k=1;k<=sum;k++)
      {
          if(sum==(i+j+k))
          {
              if(i<j&&j<k&&i<k&&(i*i+j*j==k*k))
              
     {
        r=i*j *k;
     }
          }
      }
      System.out.println (r);
      
  
 
       }
}
